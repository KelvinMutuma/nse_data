<h1>Listing share_indices</h1>

<table>
  <thead>
    <tr>
      <th>All share index</th>
      <th>Twenty share index</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @share_indices.each do |share_index| %>
      <tr>
        <td><%= share_index.all_share_index %></td>
        <td><%= share_index.twenty_share_index %></td>
        <td><%= link_to 'Show', share_index %></td>
        <td><%= link_to 'Edit', edit_share_index_path(share_index) %></td>
        <td><%= link_to 'Destroy', share_index, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Share index', new_share_index_path %>
